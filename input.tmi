read-a-char port k = (Command ["read-char", port] k)
read-file filename =
  Command ["open-input-file", filename]
    (/. (port) (Command ["shew", port]
    (/. (void) (read-a-char port
    (/. (c) (read-a-char port
    (/. (c2) (Command ["pretty-shew", [c, c2]]
    (/. (v) (Command (Done)))))))))))
main = read-file "g"
